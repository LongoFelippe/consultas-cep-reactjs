{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018
  },
  "plugins": ["react"],
  "rules": {
    "import/prefer-default-export": "off",
    "react/forbid-prop-types": 0,
    "react/prop-types": 0,
    "linebreak-style": 0,
    "max-len": ["error", 200, 2]
  }
}
